    "react-dom": "^16.8.2",
    "react-flexbox-grid": "^2.1.2",
    "react-ga": "^2.5.7",
    "react-redux": "^7.0.2",
    "react-router": "^5.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "scriba": "^1.0.5",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "uuid": "^3.3.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  },
  "pre-commit": [
    "lint:soft"
  ],
  "xo": {
    "parser": "babel-eslint",
    "space": true,
    "extends": [
      "xo-react/space",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "babel",
      "flowtype"
    ],
    "rules": {
      "babel/semi": true,
      "react/jsx-fragments": [
        true,
        "element"
      ]
    },
    "settings": {
      "react": {
        "version": "16.5"
      }
    },
    "env": [
      "browser",
      "node"
    ],
    "ignore": [
      "static/javascripts/**/*.js"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "helpers": [
      "test/helpers/**/*.js"
    ],
    "require": [
      "@babel/register",
      "@babel/polyfill",
      "./test/helpers/setup-browser-env",
      "./test/helpers/setup-enzyme",
      "ignore-styles"
    ]
  }
}
